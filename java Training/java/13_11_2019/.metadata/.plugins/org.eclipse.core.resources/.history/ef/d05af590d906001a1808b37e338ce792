package com.test;

import java.io.IOException;

import com.fileserialization.FileOpeartion;
import com.pojo.Dog;

public class DogTest {

	public static void main(String[] args) throws IOException, ClassNotFoundException {
		
		Dog dog = new Dog("Tommy", 12, 300);
		
		FileOpeartion obj = new FileOpeartion();
		obj.serialiazeObject(dog);
		obj.deserizedObject();
		
	}

}
